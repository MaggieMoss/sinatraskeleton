<% @messages.each do |message| %>
  <div class="message">
    <h3><a href="/messages/<%= message.id %>"> <%= "to message" %> </a>
    	</br>
    	<small>(by: <%= message.author %>)</small>
    	<small>(url: <%= message.url %>) </small>
    	<small>(created at: <%= message.created_at %>)</small>
    </h3>
    <p>
      <%= message.content %>
    </p>
  </div>
<% end %>